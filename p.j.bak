{
  "name": "@haftahave/serverless-ses-template",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "5.0.0",
  "description": "Serverless template sync plugin for Amazon Simple Email Service",
  "keywords": [
    "serverless",
    "AWS SES",
    "Amazon Simple Email Service",
    "plugin"
  ],
  "homepage": "https://github.com/Enase/serverless-ses-template",
  "bugs": {
    "url": "https://github.com/Enase/serverless-ses-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Enase/serverless-ses-template.git"
  },
  "license": "MIT",
  "author": "Sergii Kovalov <enasik@gmail.com> (https://www.linkedin.com/in/enase)",
  "maintainers": [
    {
      "name": "Sergii Kovalev",
      "email": "enasik@gmail.com"
    }
  ],
  "contributors": [
    {
      "name": "Arseny Yankovsky"
    },
    {
      "name": "Arseny Yankovsky",
      "email": "mike@cousins.io"
    }
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "scripts": {
    "build:clean": "shx rm -rf ./dist",
    "build:package": "npm-run-all build:clean compile",
    "compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.types.json",
    "prepublishOnly": "npm run build:package && npm run typecheck && npm run lint && npm run test",
    "lint": "eslint .",
    "typecheck": "tsc -noEmit",
    "test": "pnpm exec jest"
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "peerDependencies": {
    "serverless": ">=3.2.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@tsconfig/node16": "16.1.1",
    "@types/cli-table": "^0.3.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^16.18.48",
    "@types/serverless": "^3.12.15",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "nock": "^13.3.3",
    "serverless": "^3.34.0",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.1.1",
    "typescript": "5.1.6"
  },
  "files": [
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "dist/types/**/!(*.tsbuildinfo)",
    "LICENSE",
    "*.md"
  ],
  "engines": {
    "node": "^12.17.0 || ^14.13 || >=16.0.0"
  }
}
