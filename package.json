{
  "name": "@haftahave/serverless-ses-template",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "4.0.7",
  "type": "module",
  "description": "Serverless template sync plugin for Amazon Simple Email Service",
  "keywords": [
    "serverless",
    "AWS SES",
    "Amazon Simple Email Service",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/Enase/serverless-ses-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Enase/serverless-ses-template.git"
  },
  "license": "MIT",
  "author": "Sergii Kovalov <enasik@gmail.com>",
  "maintainers": [
    "Sergii Kovalev <enasik@gmail.com> (https://github.com/Enase)"
  ],
  "contributors": [
    "Arseny Yankovsky (https://github.com/ArsenyYankovsky)",
    "Mike Cousins <mike@cousins.io> (https://mike.cousins.io)"
  ],
  "main": "src/index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "coverage_report": "nyc report --reporter=html",
    "coverage_test": "nyc check-coverage --lines 80 --functions 80 --branches 70",
    "lint": "eslint .",
    "typecheck": "tsc -noEmit",
    "test": "nyc ./node_modules/mocha/bin/_mocha tests -R spec --recursive"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "examples/*",
      ".eslintrc.js",
      "tests/*",
      "coverage/*"
    ],
    "report-dir": "./coverage"
  },
  "peerDependencies": {
    "serverless": ">=3.0.0"
  },
  "dependencies": {
    "cli-table": "^0.3.11",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "16.18.39",
    "@types/serverless": "^3.12.13",
    "@types/cli-table": "^0.3.1",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "serverless": "^3.34.0",
    "sinon": "^15.2.0",
    "typescript": "5.1.6",
    "@tsconfig/node16": "16.1.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-config-prettier": "^8.10.0"
  },
  "files": [
    "src/",
    "LICENSE",
    "*.md"
  ],
  "engines": {
    "node": ">= 16.17.1"
  }
}
